<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.veer.custom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.WebviewActivity"/>
        <activity android:name=".ui.FillingviewActivity"/>
        <activity android:name=".ui.WaveWaterActivity"/>
        <activity android:name=".ui.WaveWater2Activity"/>
        <activity android:name=".ui.WaveWater3Activity"/>
        <activity android:name=".ui.JellyActivity"/>
        <activity android:name=".ui.SwipeRefreshActivity"/>
        <activity android:name=".ui.SmoothProgressActivity"/>
        <activity android:name=".ui.RoundedRectProgressActivity"/>
        <activity android:name=".test.TestActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>